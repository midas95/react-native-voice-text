{
  "name": "voice-scribe",
  "version": "1.0.0",
  "description": "Mobile app for the voice-scribe application",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "contributors": [
    "https://github.com/cmcWebCode40"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cmcWebCode40/voice-scribe-mobile"
  },
  "bugs": {
    "url": "https://github.com/cmcWebCode40/voice-scribe-mobile/issues"
  },
  "keywords": [
    "react-native",
    "javascript",
    "react",
    "yarn",
    "expo",
    "expo eas"
  ],
  "scripts": {
    "start": "expo start",
    "type:check": "yarn tsc",
    "format:check": "npx prettier . --check",
    "format": "npx prettier . --write",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clean:node_modules": "rm -rf node_modules/",
    "build:ios": "eas build --development --platform ios",
    "build:android": "eas build --development --platform android"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "@supabase/supabase-js": "^2.8.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "expo": "~47.0.12",
    "expo-application": "~5.0.1",
    "expo-device": "~5.0.0",
    "expo-file-system": "~15.1.1",
    "expo-localization": "~14.0.0",
    "expo-speech": "^11.0.0",
    "expo-splash-screen": "^0.17.5",
    "expo-status-bar": "~1.4.2",
    "formik": "^2.2.9",
    "i18next": "^22.4.9",
    "posthog-react-native": "^2.5.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "^12.1.5",
    "react-native": "0.70.5",
    "react-native-document-picker": "^8.1.3",
    "react-native-error-boundary": "^1.2.1",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-web": "~0.18.9",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "3.0.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.9.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "^4.6.3"
  }
}
